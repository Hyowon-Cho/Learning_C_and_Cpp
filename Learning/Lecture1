#include <stdio.h>

int main() {
  printf("%d + %d = %d\n", 2, 3, 5);
  
  printf("%f\n", 3.14);
  printf("%.5f\n", 3.141592);

  printf("%g\n", 3.141592);

  printf("%c %c %c\n", 'a', 'b', 'c');
  
  printf("%s\n", "Hello");

}

// %d : Interger Output
// %f  : Float Output
// %g : Real Output (also output in exponential form)
// %c : Character output (alphabet, number, some symbols)
// %s : String Output


